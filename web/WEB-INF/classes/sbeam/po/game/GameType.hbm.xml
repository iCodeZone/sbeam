<?xml version="1.0"  encoding="utf-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="sbeam.po.game.GameType" table="gametype" catalog="sbeam">
        <id name="id" column="gtid" type="java.lang.Integer"><generator class="increment"/></id>
        <property name="name" column="gtname" type="java.lang.String"/>
    </class>
</hibernate-mapping>