<?xml version="1.0"  encoding="utf-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="sbeam.po.game.GameAndType" table="gameandgametype" catalog="sbeam">
        <composite-id>
            <key-property name="gameId" type="java.lang.Integer" column="gid"/>
            <key-property name="gameTypeId" type="java.lang.Integer" column="gtid"/>
        </composite-id>
    </class>
</hibernate-mapping>